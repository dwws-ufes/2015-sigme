<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.0" xmlns="http://java.sun.com/xml/ns/javaee"
 xmlns:xi="http://www.w3.org/2001/XInclude"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
 <navigation-rule>
  <navigation-case>
   <from-outcome>logout</from-outcome>
   <to-view-id>/logout</to-view-id>
  	<redirect/>
  </navigation-case>
 </navigation-rule>
 <application>
  <message-bundle>br.org.feees.sigme.faces</message-bundle>
  <resource-bundle>
   <base-name>br.org.feees.sigme.messages</base-name>
   <var>msgs</var>
  </resource-bundle>
  <resource-bundle>
   <base-name>br.org.feees.sigme.core.view.messages</base-name>
   <var>msgsCore</var>
  </resource-bundle>
  <resource-bundle>
   <base-name>br.org.feees.sigme.secretariat.view.messages</base-name>
   <var>msgsSecretariat</var>
  </resource-bundle>
  <!-- Defines the default locale. -->
  <locale-config>
   <default-locale>pt-BR</default-locale>
  </locale-config>
 </application>
</faces-config>
